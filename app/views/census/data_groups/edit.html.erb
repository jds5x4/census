<%= form_for [:census, @data_group], :url => {:action => :update, :controller => :data_groups}, :html => { :class => "census_data_group_form" } do |form| %>
<fieldset>
  <legend>Edit Census Data Group</legend>
  <%= render :partial => 'form', :object => form %>
  <%= form.submit 'Save', :disable_with => 'Please wait...' %>
</fieldset>
<% end %>
